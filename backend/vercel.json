{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/api/balance",
      "dest": "/api/balance.ts"
    },
    {
      "src": "/api/txs",
      "dest": "/api/txs.ts"
    },
    {
      "src": "/api/send-tx",
      "dest": "/api/send-tx.ts"
    },
    {
      "src": "/",
      "dest": "/index.ts"
    }
  ]
}

